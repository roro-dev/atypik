{% extends 'base-bo.html.twig' %}
{% block title %}Création d'utilisateur{% endblock %}
{% block body %}
<div class="container">    
    <div class="row mx-auto my-2">
        <a href="{{ path('logement_liste') }}">Retour</a>
    </div>
    <h1>Création d'un utilisateur</h1>
    {% for flash_message in app.session.flashBag.get('error') %}
        <div class="alert alert-error">
            {{ flash_message }}
        </div>
    {% endfor %} 
    {{ form_start(form) }}
        {{ form_widget(form) }}
        <button class="btn">{{ button_label|default('Créer') }}</button>
    {{ form_end(form) }}
    </div>
</div>
{% endblock %}